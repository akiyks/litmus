C C-wo+ro-mb-ro+wo-mb-ro
(* Forbid, all full memory barriers. *)
{
x = 0;
y = 0;
}

P0(int *x)
{
	WRITE_ONCE(*x, 1);
}

P1(int *y, int *x)
{
	r1 = READ_ONCE(*x);
	smp_mb();
	r2 = READ_ONCE(*y);
}

P2(int *x, int *y)
{
	WRITE_ONCE(*y, 1);
	smp_mb();
	r4 = READ_ONCE(*x);
}

exists (1:r1=1 /\ 1:r2=0 /\ 2:r4=0)
